{
  "name": "resume-it",
  "version": "1.0.0",
  "description": "Build Resume with different templates",
  "main": "index.js",
  "engines": {
    "node": "18.18.0",
    "npm": "9.8.1"
  },
  "scripts": {
    "client-install": "npm install --prefix app",
    "server": "nodemon server/server.js",
    "client-build": "npm run build --prefix app",
    "start": "node server.js",
    "start-client": "npm start --prefix app",
    "dev": "concurrently \"npm run client-build\"  \"npm run server\" \"npm run start-client\""
    
  },
  "author": "Santosh Pandhare",
  "license": "ISC",
  "dependencies": {
    "antd": "^5.9.4",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "js-cookie": "^3.0.5",
    "mongoose": "^7.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
}
